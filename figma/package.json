{
  "name": "figma",
  "description": "A web hosted version of figma plugin.",
  "authors": "Grida.co",
  "version": "0.0.0",
  "private": false,
  "dependencies": {
    "@ui/skeleton": "0.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn clean && webpack --mode=production --host=production && yarn optimize",
    "optimize": "node scripts/optimize.js",
    "build:staging": "yarn clean && webpack --mode=production --host=staging",
    "build:dev": "webpack --mode=development --host=dev",
    "watch": "webpack --watch",
    "dev": "yarn watch"
  },
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "react-dev-utils": "^12.0.1"
  }
}
